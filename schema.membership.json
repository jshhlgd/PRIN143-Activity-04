{
    "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://example.com/schema.student.json",
  "title": "Membership",
    "Membership": {
      "LastName": {
        "type": "string",
        "minLength": 2,
        "maxLength": 20
      },
      "GivenName": {
        "type": "string",
        "minLength": 2,
        "maxLength": 20
      },
      "MiddleName": {
        "type": "string",
        "nullable": true
      },
      "Birthdate": {
        "Year": {
          "type": "integer",
          "minimum": 1900,
          "maximum": 2025
        },
        "Month": {
          "type": "integer",
          "minimum": 1,
          "maximum": 12
        },
        "Day": {
          "type": "integer",
          "minimum": 1,
          "maximum": 31
        }
      },
      "Gender": {
        "type": "string",
        "enum": ["M", "F"]
      },
      "ContactDetails": {
        "MobileNumber": {
          "type": "string",
          "pattern": "^0[0-9]{10}$"
        },
        "EmailAddress": {
          "type": "string",
          "format": "email"
        }
      },
      "Interests": {
        "type": "array",
        "minItems": 1,
        "items": {
          "type": "string"
        }
      }
    },
    "Platforms": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "properties": {
          "Platform": {
            "type": "string",
            "enum": ["PC", "Playstation", "XBOX"]
          },
          "Enabled": {
            "type": "boolean"
          }
        },
        "required": ["Platform", "Enabled"]
      }
    }
  }